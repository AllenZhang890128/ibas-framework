<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app>
<web-app>
	<display-name>bobas.demo</display-name>
	<welcome-file-list>
		<welcome-file>testServices.html</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>bobas.demo</servlet-name>
		<servlet-class>org.glassfish.jersey.servlet.ServletContainer
		</servlet-class>
		<init-param>
			<param-name>jersey.config.server.provider.packages</param-name>
			<param-value>bobas.businessobjectscommon.demo.repository
			</param-value>
		</init-param>
		<init-param>
			<param-name>jersey.config.server.provider.classnames</param-name>
			<param-value>org.glassfish.jersey.media.multipart.MultiPartFeature
			</param-value>
		</init-param>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>bobas.demo</servlet-name>
		<url-pattern>/services/*</url-pattern>
	</servlet-mapping>
</web-app>
